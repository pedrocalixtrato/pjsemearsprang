CREATE TABLE TESTE_PESSOA (
  ID INTEGER UNSIGNED  NOT NULL   AUTO_INCREMENT,
  ID_PESSOA_FISICA BIGINT(20)  NOT NULL  ,
  TIPO INTEGER UNSIGNED  NULL   COMMENT '0-Residencial|1-Comercial|2-Celular|3-Outro' ,

PRIMARY KEY(ID, ID_PESSOA_FISICA)  ,

INDEX FK_PESSOA_FISICA(ID_PESSOA_FISICA ASC),

  FOREIGN KEY(`ID_PESSOA_FISICA`)
    REFERENCES pessoa_fisica(`ID`)
      ON DELETE NO ACTION
      ON UPDATE NO ACTION)
ENGINE = InnoDB
  DEFAULT CHARSET = utf8MB4;
  
  
